DATA Aap   |  Een  a : {Int}
                   b : {Int}
           |  Twee c : {Int}
                   d : {Int}
                   e : Noot
                   
DATA Noot  |  Tien a : Aap
                   b : {Int}


ATTR Aap Noot [ | | beide : {Int} ]

ATTR Aap [ | | primair : {Int} ]

ATTR Noot [ inh : {Int} | | ]

SEM Aap
  | Een  lhs.beide = @a
         lhs.primair = @b
  | Pipo lhs.beide = @b  -- undefined constructor
  | Twee lhs.beide = @x  -- undefined child
         lhs.(primair,primair) = (@d,@d)
         lhs.extra  = @d
         e.inh = 5
         e.exinh = 6

SEM Noot
  | Tien lhs.beide = @b
  
  
  

--multiply defined attribute  
SEM Aap
  | Een  lhs.beide = @b

--triple defined
SEM Aap
  | Een  lhs.beide = @a


-- undefined constructor
SEM Aap
  | Nono lhs.beide = @b


-- undefined nonterminal
SEM Raar
  | Hoi  lhs.beide = @b 

