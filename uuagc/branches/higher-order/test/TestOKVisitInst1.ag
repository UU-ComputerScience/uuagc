-- A grammar that requires 2 visits and uses an inst attribute

DATA Root
   | Root

DATA Aap
	| Een

ATTR Root [ | | z : Int ]
ATTR Aap [ x : Int | | y : Int z : Int ]

SEM Root
  | Root inst.a : Aap
         inst.a = Aap_Een
         -- a.x = @a.y
         a.x = 3
          
SEM Aap
  | Een  lhs.y = 7
         lhs.z = @lhs.x

WRAPPER Root
